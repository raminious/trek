{
  "extends": [
    "@commitlint/config-conventional",
    "@commitlint/config-angular"
  ],
  "rules": {
    "header-case": [
      0
    ],
    "type-enum": [
      2,
      "always",
      [
        "build",
        "chore",
        "ci",
        "docs",
        "feat",
        "fix",
        "perf",
        "refactor",
        "revert",
        "style",
        "test"
      ]
    ]
  }
}